<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SSSorular">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:fontFamily="@font/poppinsbold"
        android:gravity="center"
        android:text="Sık Sorulan Sorular"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="1.Kişisel bilgilerim güvende mi?
                              Evet kesinlikle. Kişisel bilgileriniz ve size ait olan tüm bilgiler güvenli olarak saklanmaktadır. Sizden başka hiç kimsenin ulaşması mümkün değildir."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15">

    </MultiAutoCompleteTextView>

    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="2.Uygulamada yayınlanan turlar güncel mi?        Sizlere daha iyi hizmet verebilmek adına tur bilgileri mevsimsel olarak güncellenmektedir."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/multiAutoCompleteTextView"></MultiAutoCompleteTextView>

    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="3.Bulunduğum konuma göre mevcut yerleri görebilir miyim?                                                           Uygulama içerisinde harita aracılığı ile gezebileceğiniz yerleri görmeniz mümkündür."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/multiAutoCompleteTextView2"></MultiAutoCompleteTextView>

    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="4.Gezmek istediğim yerleri bir daha nasıl bulabilirim?                                                           Uygulamada istediğiniz yer ve mekanları beğenip kaydedebilirsiniz. Böylelikle daha sonrasında kaydetmiş ve beğenmiş olduğunuz yerleri görebilirsiniz."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/multiAutoCompleteTextView3"></MultiAutoCompleteTextView>

    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="5.Yer ve mekan hakkında nasıl bilgiye ulaşabilirim?                                                                    Yer ve mekanlar hakkında bir çok bilgiyi paylaşmaya çalışıyoruz. Bunun dışında yapılan yorumlar sayesinde fikir sahibi olabilirsiniz."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"></MultiAutoCompleteTextView>


</androidx.constraintlayout.widget.ConstraintLayout>